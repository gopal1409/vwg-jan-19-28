pipeline {
    agent any
    stages {
        stage('code from scm') {
            steps {
               git credentialsId: 'gitkey', url: 'git@github.com:gopal1409/vw-jenkins-manifest-jan26.git'
            }
        }
        stage('mvn build') {
            steps {
                sh 'mvn -B -DskipTest clean package'
            }
        }
        stage('mvn test') {
            steps {
                sh 'mvn test'
                junit 'target/surefire-reports/*.xml'
            }
        }
    }
}
