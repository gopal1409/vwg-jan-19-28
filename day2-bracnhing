 499  cd project
   #this will show all the branches
  500  git branch
   #lets create a new branch
  501  git branch subtract
  502  git branch
   #to move to the branch
  503  git checkout subtract
  504  git branch
   #we will add a new line in subtract branch
  505  cat calcy.py
  506  echo "we are developing subtract functionality" >> calcy.py
  507  cat calcy.py
  508  git add calcy.py
  509  git commit -m "added subtract function"
   #git log will show the header is in subtract branch
  510  git log
  511  cat calcy.py
   #lets merge the subtract branch with master branch
  512  git checkout master
  513  cat calcy.py
  514  git merge subtract
  515  cat calcy.py
#lets create a conflict
 518  git branch
  519  cat calcy.py
#we are adding a new line in master branch
  520  echo "lets create a conflict" >> calcy.py
  521  git add calcy.py
  522  git commit -m "conflict"
 #then we move to subtract branch
  528  git checkout subtract
#on the same file we added a new line with different content
  529  cat calcy.py
  530  echo "testing conflict" >> calcy.py
  531  cat calcy.py
  532  git add calcy.py
  533  git commit -m "conflict arise"
#lets move to the master 
  534  git checkout master
#if we try to merge it it will show you the conflict
  535  git merge subtract
