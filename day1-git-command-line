#lets create some global varaibles you need to change my name to your name
   501  git config --global user.name "gopal"
  502  git config --global user.email "gopal1409@gmail.com"
  #this will show all the global varaible list
  503  git config --list
#lets undestand git init what it do
506  mkdir project
  507  cd project
#touch command will create empty file
  508  touch DS_Store
  509  touch auth
  510  touch secret
  511  touch calcy.py
  512  ls
  513  git status
#to manage this folder using git you can give a command git init
  514  git init
  515  git status
  516  ls -la
#git5 init will create a folder called as .git which will be used to manage your versioing
  517  ls .git
#if you delete .git folder all your versioingh will be lost
  518  rm -rf .git
  519  git status
#lets reintizlied it 
  523  git init
  524  git status
  525  echo "we are starting with calcy project" > calcy.py
#we will add calcy.py
  526  git add calcy.py
  527  git status
#do a commit which will create my first version
  528  git commit -m "intialized calcy project"
  529  git status
  530  ls
#git log will show me the first version which we have created just now
  531  git log
